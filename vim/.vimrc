set nocompatible
set tabstop=4
set softtabstop=4
set shiftwidth=4
syntax on
filetype on
filetype plugin on
filetype indent on
set smartindent
set expandtab
set number
set incsearch
set ignorecase
set showmatch
set hlsearch
set showcmd
set showmode
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
colorscheme molokai
